    "@typescript-eslint/parser": "5.59.7",
    "@vitest/coverage-c8": "0.31.1",
    "del-cli": "5.0.0",
    "eslint": "8.41.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "tsx": "3.12.7",
